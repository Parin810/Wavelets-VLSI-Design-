#include "stdio.h"
#include "io.h"
#include "system.h"

int main(void)
{
	unsigned int a=0,b=6,c,ans;
	int i;
	c=(a << 16)+b;
	IOWR(DAC_RAM_0_BASE,0,c);
	//printf("%d",c);
	for(i=0;i<30;i++)
	{
		printf("asdsa");
	}
	ans=IORD(DAC_RAM_0_BASE,0);
	printf("\n%d",ans);
	while(1);

}

